# program to return/print the string that has max character in list
list1 = ["python", "programming", "abc"]
print("The given string list is:",list1)
charcount = []
num1 = 1
for i in list1:
    count = 0
    for j in i:
        count += 1
    charcount.append(count)
# print(charcount)
largest = charcount[0]
for i in charcount:
    if i > largest:
        largest = i
# print(largest)
# print(charcount.index(largest))
largstr = charcount.index(largest)
print("The string that has max character in it :",list1[largstr])
